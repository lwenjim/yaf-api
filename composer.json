{
    "name": "lwenjim/yaf-framework",
    "description": "The Lwenjim Yaf Framework",
    "keywords": [
        "yaf-framework"
    ],
    "license": "MIT",
    "support": {
        "issues": "https://github.com/lwenjim/yaf-api/issues",
        "source": "https://github.com/lwenjim/yaf-api"
    },
    "authors": [
        {
            "name": "jim",
            "email": "lwenjim@163.com"
        }
    ],
    "require": {
        "ext-PDO": "^7.1",
        "ext-curl": "^7.1",
        "ext-json": "^1.5",
        "ext-yaf": "^3.0",
        "illuminate/database": "5.5.*",
        "illuminate/events": "5.5.*",
        "illuminate/pagination": "5.5.*",
        "illuminate/validation": "5.5.*",
        "illuminate/config": "5.5.*",
        "league/fractal": "0.18.0",
        "lwenjim/log": "1.0.x-dev",
        "particle/validator": "2.3.3",
        "vlucas/phpdotenv": "~2.2",
        "guzzlehttp/guzzle": "6.3.3"
    },
    "require-dev": {
        "phpunit/phpunit": "7.5.9"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Lwenjim\\Yaf\\": "src/"
        },
        "files": [
            "src/helps.php"
        ]
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
